
<div class="row">

</div>
<div class="row">
    <div class="col s12 m9">
        <div class="row">
            <div class="input-field col s12 m12 onehd">
                <input placeholder="User Tag" id="user-tag" type="text" class="validate" required>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12 m12 onehd">
                <input placeholder="First Name" id="first-name" type="text" class="validate" required>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12 m12 onehd">
                <input placeholder="Last Name" id="last-name" type="text" class="validate" required>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12 m12 onehd">
                <input placeholder="Password" id="new-password" type="password" class="validate" required>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12 m12 onehd">
                <input placeholder="E-Mail" id="new-email" type="email" class="validate" required>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12 m12 onehd">
                <input placeholder="Confirm E-Mail" id="v-new-email" type="email" class="validate" required>
            </div>
        </div>
        <div class="row">
            <div class="col s12 m12 onehd">
                <button  class="waves-effect waves-light btn teal darken-3" id="create-account">
                    <i class="material-icons left">edit</i>Create Account
                </button>
            </div>
        </div>
    </div>
</div>


